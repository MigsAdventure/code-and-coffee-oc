{
  "name": "code-and-coffee-oc",
  "version": "0.1.0",
  "homepage": ".",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --progress --config webpack.config.dev.js",
    "build": "webpack -p --progress --config webpack.config.build.js",
    "build-css": "node-sass-chokidar src/styles/ -o src/_styles",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/ -o src/_styles --watch --recursive",
    "deploy": "npm run build&&gh-pages -d build",
  },
  "dependencies": {
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.18",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "ejs-loader": "^0.3.0",
    "file-loader": "^1.1.9",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.2"
  }
}
